public class Regalo {
    public static String elegirRegalo(int vida) {
        String gift = "";
        switch(vida){
            case 1: gift = "Una entrada al cine"; break; 
            case 2: gift = "Una visita al museo"; break; 
            case 3: gift = "Un pasaje al caribe"; break; 
        }
        return gift;
    }
}
